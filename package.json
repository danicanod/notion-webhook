{
  "name": "notion-webhook-server",
  "version": "1.0.0",
  "description": "Servidor de webhooks para Notion - Production ready",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "nodemon --exec tsx src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["notion", "webhooks", "render", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
