# Puerto del servidor (Railway usa PORT automáticamente)
PORT=3000

# Token de integración de Notion
NOTION_TOKEN=secret_xxxxxxxxxxxxxxxxx

# NOTA: WEBHOOK_SECRET ya no se usa. El verification_token se obtiene automáticamente de Notion

# ID de la base de datos de días
DAY_DATABASE_ID=xxxxxxxxxxxxxxxxx

# URL base del sistema principal (opcional)
MAIN_SYSTEM_URL=http://localhost:3001

# Configuración de logs
LOG_LEVEL=info 